<% if (title) { %>
    <h1><%= title %></h1>
  <% } %>
  
  <%- messages() %>
  
  <% if (errors) { %>
    <ul class="notice">
      <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>
  
  <div class="account-management">
      <h2>Welcome, <%= accountData.account_firstname %></h2>
      <p>You're logged in!</p>
      <p>Account Type: <%= accountData.account_type %></p>

      <a href="/account/logout" class="btn-logout">Logout</a>
  </div>